{
  "name": "icecrawl",
  "version": "0.3.16",
  "description": "Web scraping application with HTTP API (incl. Dashboard), CLI, and MCP Server interfaces.",
  "main": "dist/index.js",
  "bin": {
    "icecrawl": "dist/cli.js",
    "icecrawl-mcp": "dist/mcp-server.js"
  },
  "files": [
    "dist",
    "prisma/schema.prisma",
    "prisma/seed.js",
    "prisma/seed.js.map",
    "prisma/migrations",
    "README.md",
    "LICENSE",
    "package.json",
    "scripts"
  ],
  "scripts": {
    "build": "tsc && tsc prisma/seed.ts --outDir prisma --module commonjs --target es2020 --resolveJsonModule true --esModuleInterop true --sourceMap true --skipLibCheck true",
    "build:dashboard": "xcopy src\\dashboard public\\dashboard /E /I /Y",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "jest",
    "test:quick": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "postinstall": "node scripts/global-postinstall.js",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "analyze-version": "node scripts/version-analyzer.js"
  },
  "keywords": [
    "webscraper",
    "cli",
    "api",
    "typescript",
    "nodejs",
    "crawler",
    "scraper",
    "mcp"
  ],
  "author": "Ambient Flare <admin@ambientflare.com> (https://ambientflare.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangdangel/icecrawl.git"
  },
  "homepage": "https://ambientflare.com/opensource/icecrawl",
  "bugs": {
    "url": "https://github.com/wangdangel/icecrawl/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.565.0",
    "@modelcontextprotocol/sdk": "1.8.0",
    "@prisma/client": "^5.11.0",
    "prisma": "^5.11.0",
    "@types/turndown": "5.0.5",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "16.4.7",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.12",
    "puppeteer": "^22.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "turndown": "7.2.0",
    "winston": "^3.12.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.2.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/http-proxy-agent": "^4.0.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.8",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "3.1.9",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,js,json,md}": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
